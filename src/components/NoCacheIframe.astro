---
interface Props {
  url: string;
  width?: string;
  height?: string;
}

const { url } = Astro.props;
const finalUrl = `${url}?t=${Date.now()}`;
---

<iframe
  class="project-iframe"
  src={finalUrl}
  style="border: none;"
/>

<style>
.project-iframe {
  width: 100%;
  height: 670px;
}

/* Mobile */
@media (max-width: 768px) {
  .project-iframe {
    height: 570px;
  }
}


</style>